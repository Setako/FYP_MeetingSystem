{
    "name": "meeting-device-ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "dev": "concurrently \"npm run serve\" \"npm run devstart\"",
        "start": "tsc --module commonjs && electron .",
        "devstart": "tsc --module commonjs && electron . -u http://localhost:4200 -d",
        "serve": "ng serve",
        "build": "ng build --base-href ./",
        "devbuild": "ng build --watch --base-href ./",
        "postinstall": "ng build --base-href ./",
        "rebuild": "npm rebuild --runtime=electron --target=4.0.6 --disturl=https://atom.io/download/atom-shell --abi=69",
        "format": "prettier --write ./**/**/*.{ts,js,json,html,css}",
        "lint": "tslint -p tsconfig.json -c tslint.json"
    },
    "main": "dist/out-tsc/app.js",
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.0.0-beta.6",
        "@angular/cdk": "^7.0.0-rc.2",
        "@angular/common": "^8.0.0-beta.6",
        "@angular/compiler": "^8.0.0-beta.6",
        "@angular/core": "^8.0.0-beta.6",
        "@angular/forms": "^8.0.0-beta.6",
        "@angular/http": "^8.0.0-beta.6",
        "@angular/material": "^7.0.0-rc.2",
        "@angular/platform-browser": "^8.0.0-beta.6",
        "@angular/platform-browser-dynamic": "^8.0.0-beta.6",
        "@angular/platform-server": "^8.0.0-beta.6",
        "@angular/router": "^8.0.0-beta.6",
        "bootstrap": "^4.3.1",
        "commander": "^2.19.0",
        "concurrently": "^4.1.0",
        "core-js": "^2.6.5",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "qrcode": "^1.3.3",
        "robotjs": "^0.5.1",
        "rxjs": "^6.4.0",
        "zone.js": "^0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.14.0-beta.4",
        "@angular/cli": "^8.0.0-beta.4",
        "@angular/compiler-cli": "^8.0.0-beta.6",
        "@angular/language-service": "^8.0.0-beta.6",
        "@types/dotenv": "^6.1.0",
        "@types/jasmine": "^3.3.9",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "~11.9.6",
        "@types/qrcode": "^1.3.1",
        "codelyzer": "~4.5.0",
        "electron": "^4.0.6",
        "jasmine-core": "~3.3.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "prettier": "^1.16.4",
        "protractor": "^5.4.2",
        "ts-node": "~8.0.2",
        "tslint": "~5.13.1",
        "typescript": "~3.2.0"
    },
    "electronWebpack": {
        "renderer": {
            "webpackConfig": "webpack.renderer.additions.js"
        }
    }
}
