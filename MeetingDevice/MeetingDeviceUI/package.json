{
    "name": "meeting-device-ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "dev": "concurrently --kill-others \"npm run serve\" \"npm run devstart\"",
        "start": "tsc --module commonjs --target esnext && electron .",
        "devstart": "concurrently \"tsc --module commonjs --target esnext && electron . -d -u http://localhost:4200\"",
        "serve": "ng serve",
        "build": "ng build --base-href ./",
        "devbuild": "ng build --watch --base-href ./",
        "rebuild": "npm rebuild --runtime=electron --target=4.0.6 --disturl=https://atom.io/download/atom-shell --abi=69",
        "format": "prettier --write ./**/**/*.{ts,js,json,html,css}",
        "lint": "tslint -p tsconfig.json -c tslint.json"
    },
    "main": "dist/out-tsc/app.js",
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@angular/cdk": "^7.0.0-rc.2",
        "@angular/http": "~7.2.0",
        "@angular/material": "^7.0.0-rc.2",
        "@nestjs/common": "^5.7.3",
        "@nestjs/core": "^5.7.3",
        "@nestjs/websockets": "^5.7.3",
        "bootstrap": "^4.3.1",
        "class-transformer": "^0.2.0",
        "class-validator": "^0.9.1",
        "commander": "^2.19.0",
        "concurrently": "^4.1.0",
        "core-js": "^2.6.5",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "ip": "^1.1.5",
        "ngx-electron": "^2.1.1",
        "qrcode": "^1.3.3",
        "reflect-metadata": "^0.1.13",
        "robotjs": "^0.5.1",
        "rxjs": "^6.4.0",
        "socket.io-client": "^2.2.0",
        "uuid": "^3.3.2",
        "zone.js": "^0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.1",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/dotenv": "^6.1.0",
        "@types/ip": "^1.1.0",
        "@types/jasmine": "^3.3.9",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "~11.9.6",
        "@types/qrcode": "^1.3.1",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "@types/uuid": "^3.4.4",
        "codelyzer": "~4.5.0",
        "electron": "^4.0.6",
        "jasmine-core": "~3.3.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "prettier": "^1.16.4",
        "protractor": "^5.4.2",
        "ts-node": "~8.0.2",
        "tslint": "~5.13.1",
        "typescript": "~3.2.0"
    },
    "electronWebpack": {
        "renderer": {
            "webpackConfig": "webpack.renderer.additions.js"
        }
    }
}
