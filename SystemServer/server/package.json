{
    "name": "conference-commander",
    "version": "1.0.0",
    "description": "description",
    "author": "",
    "license": "MIT",
    "scripts": {
        "postinstall": "npm audit fix",
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "pm2 start ecosystem.config.js",
        "start:ts": "ts-node --files -r tsconfig-paths/register src/main.ts",
        "start:dev": "nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "start:hmr": "node dist/main",
        "prestart:prod": "npm run build",
        "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "webpack": "rimraf dist/hot && webpack --config webpack.config.js"
    },
    "dependencies": {
        "@google-cloud/storage": "^2.4.3",
        "@nest-modules/mailer": "^1.1.3",
        "@nestjs/common": "^6.0.4",
        "@nestjs/core": "^6.0.4",
        "@nestjs/jwt": "^6.0.0",
        "@nestjs/mongoose": "^6.0.0",
        "@nestjs/passport": "^6.0.0",
        "@nestjs/platform-express": "^6.0.4",
        "@nestjs/platform-socket.io": "^6.0.4",
        "@nestjs/websockets": "^6.0.4",
        "class-transformer": "^0.2.0",
        "class-validator": "^0.9.1",
        "data-urls": "^1.1.0",
        "dotenv": "^7.0.0",
        "ejs": "^2.6.1",
        "fast-crc32c": "^1.0.4",
        "googleapis": "^39.1.0",
        "mongoose": "^5.4.20",
        "morgan": "^1.9.1",
        "nestjs-typegoose": "^5.1.2",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "pm2": "^3.4.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.4.0",
        "sharp": "^0.22.0",
        "socket.io": "^2.2.0",
        "typegoose": "^5.6.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^6.0.4",
        "@types/dotenv": "^6.1.1",
        "@types/ejs": "^2.6.3",
        "@types/express": "^4.16.1",
        "@types/jest": "^24.0.11",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/mongoose": "^5.3.24",
        "@types/morgan": "^1.7.35",
        "@types/node": "^11.12.2",
        "@types/nodemailer": "^4.6.7",
        "@types/passport-jwt": "^3.0.1",
        "@types/sharp": "^0.22.1",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "@types/supertest": "^2.0.7",
        "@types/uuid": "^3.4.4",
        "fork-ts-checker-webpack-plugin": "^1.0.1",
        "jest": "^24.5.0",
        "nodemon": "^1.18.10",
        "prettier": "^1.16.4",
        "rimraf": "^2.6.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.0.0",
        "ts-loader": "^5.3.3",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.8.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^5.14.0",
        "typescript": "^3.4.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-node-externals": "^1.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "heroku-run-build-script": true
}
