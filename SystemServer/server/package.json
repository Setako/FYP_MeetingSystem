{
    "name": "conference-commander",
    "version": "1.0.0",
    "description": "description",
    "author": "",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/main.js",
        "start:ts": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "webpack": "webpack --config webpack.config.js",
        "postinstall": "npm run prestart:prod"
    },
    "dependencies": {
        "@nestjs/common": "^5.4.0",
        "@nestjs/core": "^5.4.0",
        "@nestjs/jwt": "^0.2.1",
        "@nestjs/mongoose": "^5.2.2",
        "@nestjs/passport": "^5.1.0",
        "cache-manager": "^2.9.0",
        "class-transformer": "^0.2.0",
        "class-validator": "^0.9.1",
        "data-urls": "^1.1.0",
        "dotenv": "^6.2.0",
        "fastify": "^1.13.3",
        "fastify-formbody": "^2.0.3",
        "googleapis": "^36.0.0",
        "mongoose": "^5.4.0",
        "morgan": "^1.9.1",
        "nestjs-typegoose": "^5.1.2",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.2.2",
        "typegoose": "^5.4.1",
        "typescript": "^3.0.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^5.1.0",
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.0",
        "@types/jest": "^23.3.1",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/mongoose": "^5.3.5",
        "@types/morgan": "^1.7.35",
        "@types/node": "^10.7.1",
        "@types/passport-jwt": "^3.0.1",
        "@types/supertest": "^2.0.5",
        "@types/uuid": "^3.4.4",
        "jest": "^23.5.0",
        "nodemon": "^1.18.3",
        "prettier": "^1.14.2",
        "supertest": "^3.1.0",
        "ts-jest": "^23.1.3",
        "ts-loader": "^5.3.1",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.5.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^5.12.0",
        "webpack": "^4.28.1",
        "webpack-cli": "^3.1.2",
        "webpack-node-externals": "^1.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
